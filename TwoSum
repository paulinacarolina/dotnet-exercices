public class Solution {
    public int[] TwoSum(int[] nums, int target) {
        List<int> result = new List<int>();
        for(int i = 0 ; i<= nums.Length-1; i++){
            for (int j = i + 1; j < nums.Length; j++)
            if(nums[i] + nums[j] == target){
                 return new int[] { i, j };
            }
         }
        return new int[] {};
        } 
    }


int[] nums = { 3,2,4 };
int target = 6;
TwoSum(nums, target);
